{
	"apikey": "/secure/api.key",
	"builds": [
		{
			"name": "libressl",
			"enabled": true,
			"kernels": "HARDENEDBSD LATT-SEC",
			"repo": "git://github.com/HardenedBSD/hardenedBSD-playground.git",
			"branch": "hardened/current/libressl",
			"dns": "amd64.libressl.experimental.hardened.hardenedbsd.updates.hardenedbsd.org",
			"src_conf": "%RUNDIR%/configs/libressl.src.conf",
			"publish": {
				"method": "sftp",
				"user": "shawn",
				"host": "pkg.hardenedbsd.org",
				"directory": "/www/updates.hardenedbsd.org/pub/HardenedBSD-playground/updates/hardened/experimental/libressl/amd64"
			}
		}
	]
}
